{
  "extends": "next/core-web-vitals",
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "varsIgnorePattern": "^_$", "argsIgnorePattern": "^_$", "vars": "all" }
    ]
  }
}
